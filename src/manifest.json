{
    "name": "tana-tabs-and-links",
    "short_name": "tana-tabs-and-links",
    "description": "Renames tana tabs for a multitab experience, clips current webpage and selection to tana-paste format, and handles tana links",
    "homepage_url": "https://tana.pub/2zNR04BRXwl9/simple-browser-multi-tab-experience-for-tana",
    "version": "0.0.6",
    "version_name": "0.0.6",
    "manifest_version": 3,
    "default_locale": "en",
    "minimum_chrome_version": "88",
    "host_permissions": ["https://app.tana.inc/"],
    "permissions": [
        "scripting",
        "activeTab",
        "clipboardWrite",
        "clipboardRead",
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "128": "assets/img/clip2tana-128.png"
    },
    "action": {
        "default_icon": {
            "16": "assets/img/clip2tana-16.png",
            "24": "assets/img/clip2tana-24.png",
            "32": "assets/img/clip2tana-32.png",
            "128": "assets/img/clip2tana-128.png",
            "512": "assets/img/clip2tana-512.png"
        },
        "default_title": "__MSG_appName__"
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "open-tana-url": {
            "suggested_key": {
                "default": "Alt+Shift+T",
                "mac": "Alt+Shift+T"
            },
            "description": "Open Tana URL from clipboard"
        }
    }
}